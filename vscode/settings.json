{
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonl]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "background.backgroundOpacity": [
    0.1,
    0.1,
    0.1,
    0.1
  ],
  "background.windowBackgrounds": [
    "/home/mclement/Pictures/background/pexels-christian-heitz-285904-842711.jpg"
  ],
  "chat.commandCenter.enabled": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 20,
  "editor.fontSize": 12,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "left",
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.smartSelect.selectLeadingAndTrailingWhitespace": false,
  "editor.smartSelect.selectSubwords": false,
  "editor.smoothScrolling": true,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.jsonl": "json",
    ".vscodevimrc": "Vimscript"
  },
  "git.blame.statusBarItem.enabled": true,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": false,
    "scminput": false
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "go.toolsManagement.autoUpdate": true,
  "gopls": {
    "build.buildFlags": [
      "-tags=integration"
    ]
  },
  "jumpy2.customKeys": "fdsarewqjkluiotgbyhnc",
  "outline.showArrays": false,
  "outline.showBooleans": false,
  "outline.showConstants": false,
  "outline.showEnums": false,
  "outline.showEvents": false,
  "outline.showFields": false,
  "outline.showFiles": false,
  "outline.showInterfaces": false,
  "outline.showKeys": false,
  "outline.showModules": false,
  "outline.showNamespaces": false,
  "outline.showNull": false,
  "outline.showNumbers": false,
  "outline.showObjects": false,
  "outline.showPackages": false,
  "outline.showProperties": false,
  "outline.showStrings": false,
  "outline.showTypeParameters": false,
  "outline.showVariables": false,
  "projectManager.git.baseFolders": [
    "/home/mclement/git"
  ],
  "projectManager.git.maxDepthRecursion": 1,
  "projectManager.openInCurrentWindowIfEmpty": "never",
  "projectManager.openInNewWindowWhenClickingInStatusBar": true,
  "projectManager.projectsLocation": "/home/mclement/git",
  "roo-cline.allowedCommands": [
    "git log",
    "git diff",
    "git show",
    "npm test",
    "npm install",
    "tsc"
  ],
  "roo-cline.deniedCommands": [],
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 5000,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabs.location": "left",
  "vim.enableNeovim": true,
  "vim.highlightedyank.duration": 350,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": false,
  "vim.incsearch": true,
  "vim.leader": " ",
  "vim.smartRelativeLine": true,
  "vim.statusBarColorControl": true,
  "vim.surround": true,
  "vim.timeout": 300,
  "vim.useCtrlKeys": false,
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "window.customMenuBarAltFocus": false,
  "window.openFoldersInNewWindow": "off",
  "window.openWithoutArgumentsInNewWindow": "off",
  "window.title": "${rootName}",
  "window.zoomLevel": -1,
  "window.zoomPerWindow": false,
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#0a5206e4",
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingForeground": "#ffffff"
  },
  "workbench.colorTheme": "Default Dark+",
  "workbench.editor.showTabs": "single",
  "workbench.editor.tabActionLocation": "left",
  "workbench.iconTheme": "vscode-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 10
}
