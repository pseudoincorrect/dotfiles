{
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonl]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "background.windowBackgrounds": [
    "/home/mclement/Pictures/background/**"
  ],
  "chat.commandCenter.enabled": false,
  "codeium.enableCodeLens": false,
  "codeium.enableCommandSideHintEducation": false,
  "codeium.enableExplainProblem": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 15,
  "editor.fontSize": 12,
  "editor.minimap.showSlider": "always",
  "editor.quickSuggestionsDelay": 300,
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.smartSelect.selectLeadingAndTrailingWhitespace": false,
  "editor.smartSelect.selectSubwords": false,
  "editor.smoothScrolling": true,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "explorer.autoReveal": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.jsonl": "json",
    ".vscodevimrc": "Vimscript"
  },
  "git.blame.statusBarItem.enabled": true,
  "git.openRepositoryInParentFolders": "never",
  "go.toolsManagement.autoUpdate": true,
  "gopls": {
    "build.buildFlags": ["-tags=integration"]
  },
  "jumpy2.customKeys": "fdsarewqjkluiotgbyhnc",
  "outline.showArrays": false,
  "outline.showBooleans": false,
  "outline.showConstants": false,
  "outline.showEnums": false,
  "outline.showEvents": false,
  "outline.showFields": false,
  "outline.showFiles": false,
  "outline.showInterfaces": false,
  "outline.showKeys": false,
  "outline.showModules": false,
  "outline.showNamespaces": false,
  "outline.showNull": false,
  "outline.showNumbers": false,
  "outline.showObjects": false,
  "outline.showPackages": false,
  "outline.showProperties": false,
  "outline.showStrings": false,
  "outline.showTypeParameters": false,
  "outline.showVariables": false,
  "projectManager.git.baseFolders": [
    "/home/mclement/git",
    "/home/mclement/notes"
  ],
  "projectManager.git.maxDepthRecursion": 1,
  "projectManager.openInCurrentWindowIfEmpty": "never",
  "projectManager.openInNewWindowWhenClickingInStatusBar": true,
  "projectManager.projectsLocation": "/home/mclement/git",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 5000,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabs.location": "left",
  "vsicons.dontShowNewVersionMessage": true,
  "window.customMenuBarAltFocus": false,
  "window.openFoldersInNewWindow": "off",
  "window.openWithoutArgumentsInNewWindow": "off",
  "window.title": "${rootName}",
  "window.zoomPerWindow": false,
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#041142e4",
    "statusBar.background": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.foreground": "#ffffff",
    "statusBar.noFolderBackground": "#005f5f",
    "editorGroup.border": "#00ffff",
    "editorGroupHeader.border": "#00ffff",
    "panel.border": "#00ffff",
    "sideBar.border": "#00ffff",
  },
  "workbench.editor.showTabs": "single",
  "workbench.editor.tabActionLocation": "left",
  "workbench.iconTheme": "vscode-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 10,
  "diffEditor.renderSideBySide": false,
  "explorer.sortOrderLexicographicOptions": "unicode",
  "editor.minimap.maxColumn": 100,
  "editor.minimap.renderCharacters": false,
  "workbench.list.multiSelectModifier": "alt",
  "claudeCode.preferredLocation": "sidebar"
}
