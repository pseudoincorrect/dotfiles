[
  {
    "key": "ctrl+t",
    "command": "editor.action.smartSelect.expand"
  },
  {
    "key": "ctrl+shift+t",
    "command": "editor.action.smartSelect.shrink"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.navigateLeft"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "ctrl+shift+k",
    "command": "workbench.action.navigateUp"
  },
  {
    "key": "ctrl+shift+j",
    "command": "workbench.action.navigateDown"
  },
  {
    "key": "ctrl+o",
    "command": "workbench.action.navigateBack",
    "when": "canNavigateBack"
  },
  {
    "key": "ctrl+i",
    "command": "workbench.action.navigateForward",
    "when": "canNavigateForward"
  },
  {
    "key": "ctrl+y",
    "command": "workbench.action.openView"
  },
  {
    "key": "alt+x",
    "command": "workbench.action.openGlobalKeybindings"
  },
  {
    "key": "alt+z",
    "command": "workbench.action.openSettings"
  },
  {
    "key": "alt+q",
    "command": "projectManager.listProjects"
  },
  {
    "key": "ctrl+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "f8",
    "command": "workbench.action.terminal.clear"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quit"
  },
  {
    "key": "ctrl+1",
    "command": "workbench.files.action.focusFilesExplorer"
  },
  {
    "key": "ctrl+5",
    "command": "workbench.action.terminal.focus"
  },
  {
    "key": "ctrl+3",
    "command": "workbench.panel.chat.view.copilot.focus"
  },
  {
    "key": "ctrl+enter",
    "command": "workbench.action.edits.submit",
    "when": "chatInputHasText "
  },
  {
    "key": "enter",
    "command": "-workbench.action.edits.submit",
    "when": "chatInputHasText"
  },
  {
    "key": "f12",
    "command": "github.copilot.completions.disable"
  },
  {
    "key": "f11",
    "command": "github.copilot.completions.enable"
  },
  {
    "key": "alt+b",
    "command": "workbench.action.activityBarLocation.hide"
  },
  {
    "key": "shift+alt+b",
    "command": "workbench.action.focusActivityBar"
  },
  {
    "key": "alt+.",
    "command": "editor.action.showContextMenu",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "ctrl+o",
    "command": "-workbench.action.files.openFile",
    "when": "true"
  },
  {
    "key": "alt+d",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "escape",
    "command": "workbench.action.focusLastEditorGroup",
    "when": "terminalFocus || filesExplorerFocus || chatInputHasFocus"
  },
  {
    "key": "alt+,",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+.",
    "command": "-editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+.",
    "command": "quickInput.toggleHover",
    "when": "inQuickInput && quickInputType == 'quickPick'"
  },
  {
    "key": "ctrl+space",
    "command": "-quickInput.toggleHover",
    "when": "inQuickInput && quickInputType == 'quickPick'"
  },
  {
    "key": "ctrl+4",
    "command": "workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled"
  },
  {
    "key": "alt+w",
    "command": "workbench.action.joinAllGroups",
  },
  {
    "key": "ctrl+f",
    "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
    "when": "!activeEditorGroupEmpty"
  },
  {
    "key": "ctrl+f",
    "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+tab",
    "command": "-workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+d",
    "command": "workbench.action.quickOpenLeastRecentlyUsedEditorInGroup",
    "when": "!activeEditorGroupEmpty"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup",
    "when": "!activeEditorGroupEmpty"
  },
  {
    "key": "ctrl+d",
    "command": "workbench.action.quickOpenNavigatePreviousInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "-workbench.action.quickOpenNavigatePreviousInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+g",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+e",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "ctrl+,",
    "command": "workbench.action.quickOpen",
    "args": "@:"
  },
  {
    "key": "alt+n",
    "command": "editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+n",
    "command": "editor.action.hideHover"
  },
  {
    "key": "ctrl+1",
    "command": "-workbench.action.focusFirstEditorGroup"
  },
  {
    "key": "alt+f",
    "command": "actions.find",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "ctrl+f",
    "command": "-actions.find",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "ctrl+j",
    "command": "-workbench.action.togglePanel"
  },
  {
    "key": "alt+k",
    "command": "editor.action.insertCursorAbove",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+up",
    "command": "-editor.action.insertCursorAbove",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+j",
    "command": "editor.action.insertCursorBelow",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+down",
    "command": "-editor.action.insertCursorBelow",
    "when": "editorTextFocus"
  },
]
