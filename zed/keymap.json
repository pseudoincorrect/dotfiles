[
  {
    "bindings": {
      "alt-s": "zed::OpenSettings",
      "alt-k": "zed::OpenKeymap",
      "ctrl-'": "projects::OpenRecent",
      "ctrl-1": "project_panel::ToggleFocus",
      "ctrl-b": "tab_switcher::ToggleAll",
      "ctrl-n": "editor::FindNextMatch",
      "ctrl-p": "command_palette::Toggle",
      "ctrl-shift-n": "editor::FindPreviousMatch",
      "ctrl-t": "terminal_panel::ToggleFocus",
    },
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-r": "editor::ReloadFile",
      "ctrl-3": ["editor::ToggleComments", { "advance_downwards": false }],
      "ctrl-g ctrl-g": "editor::GoToDefinition",
      "ctrl-g ctrl-i": "editor::GoToImplementation",
      "ctrl-g ctrl-l": "go_to_line::Toggle",
      "ctrl-g ctrl-r": "editor::FindAllReferences",
      "ctrl-g ctrl-s": "outline::Toggle",
      "ctrl-g": null,
      "ctrl-h": "editor::SelectLargerSyntaxNode",
      "ctrl-i": "pane::GoForward",
      "ctrl-j": ["editor::MoveDownByLines", { "lines": 15 }],
      "ctrl-k": ["editor::MoveUpByLines", { "lines": 15 }],
      "ctrl-m": "editor::SelectLine",
      "ctrl-o": "pane::GoBack",
      "ctrl-shift-h": "editor::SelectSmallerSyntaxNode",
      "ctrl-u": "editor::DeleteLine",
    },
  },
]
